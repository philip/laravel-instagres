{
    "name": "philip/laravel-instagres",
    "description": "Laravel integration for Neon Instagres - instant PostgreSQL databases",
    "keywords": [
        "philip",
        "laravel",
        "laravel-instagres",
        "neon",
        "postgres",
        "postgresql",
        "database",
        "instagres"
    ],
    "homepage": "https://github.com/philip/laravel-instagres",
    "license": "MIT",
    "authors": [
        {
            "name": "Philip Olson",
            "email": "philip@roshambo.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "illuminate/contracts": "^10.0||^11.0||^12.0",
        "philip/instagres": "^0.1",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "larastan/larastan": "^2.9",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-arch": "^2.7",
        "pestphp/pest-plugin-laravel": "^2.3",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Philip\\LaravelInstagres\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Philip\\LaravelInstagres\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-laravel-instagres --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse --memory-limit=256M",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Philip\\LaravelInstagres\\InstagresServiceProvider"
            ],
            "aliases": {
                "Instagres": "Philip\\LaravelInstagres\\Facades\\Instagres"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
